# AIRS local development environment
# Copy to .env.dev and customize as needed.

ENV=local
APP_NAME=AIRS
DEBUG=true
PORT=8000

# Local isolated database (never production)
DATABASE_URL=sqlite:///./airs_dev.db

# Local frontend origin (Vite default)
CORS_ALLOW_ORIGINS=http://localhost:5173

# Auth behavior for local development
AUTH_REQUIRED=false
FIREBASE_AUTH_EMULATOR_HOST=127.0.0.1:9099

# LLM narrative configuration
AIRS_USE_LLM=false
DEMO_MODE=true
LLM_PROVIDER=gemini
LLM_MODEL=gemini-2.0-flash
# GEMINI_API_KEY=
